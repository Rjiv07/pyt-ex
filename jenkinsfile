pipeline {
  agent any

  stages
  {
    stage('checkout')
    {
      steps { git branch: 'main', url: "https://github.com/Rjiv07/pyt-ex.git"}
    }
    stage('build'){
      steps { echo "building...."}
    }
    stage('test'){
      steps
      {
        sh "python3 Hello.py"
      }
    }
  }  
}
